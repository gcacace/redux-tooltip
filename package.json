{
  "name": "redux-tooltip",
  "version": "0.5.2",
  "description": "A tooltip React component for Redux",
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/kuy/redux-tooltip.git"
  },
  "homepage": "https://github.com/kuy/redux-tooltip",
  "bugs": "https://github.com/kuy/redux-tooltip/issues",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --progress --color --host 0.0.0.0 --content-base examples/",
    "build": "./node_modules/.bin/babel src --out-dir lib",
    "test": "npm run test:unit && npm run test:feature:ci",
    "test:unit": "./node_modules/.bin/mocha --compilers js:espower-babel/guess --recursive tests/unit/test_*.js",
    "test:feature": "./node_modules/.bin/karma start",
    "test:feature:ci": "./node_modules/.bin/karma start karma.conf.js --single-run",
    "clean": "./node_modules/.bin/rimraf lib",
    "prepublish": "npm run clean && npm run build"
  },
  "authors": [
    "Yuki Kodama <endflow.net@gmail.com>"
  ],
  "license": "MIT",
  "keywords": [
    "redux",
    "react",
    "component",
    "tooltip",
    "react-component"
  ],
  "dependencies": {
    "dompurify": "^0.7.3",
    "is-dom": "^1.0.5",
    "react-themeable": "^1.0.1",
    "redux-actions": "^0.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.1",
    "babel-loader": "^6.2.2",
    "babel-plugin-espower": "^2.1.1",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "cssstyle": "^0.2.34",
    "deep-equal": "^1.0.1",
    "espower-babel": "^4.0.1",
    "json-loader": "^0.5.4",
    "karma": "^0.13.19",
    "karma-mocha": "^0.2.1",
    "karma-mocha-debug": "^0.1.2",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "phantomjs-prebuilt": "^2.1.4",
    "power-assert": "^1.2.0",
    "react": "^0.14.7",
    "react-addons-test-utils": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "redux": "^3.3.1",
    "redux-logger": "^2.5.2",
    "rimraf": "^2.5.1",
    "sinon": "^1.17.3",
    "webpack": "^1.12.13",
    "webpack-dev-server": "^1.14.1"
